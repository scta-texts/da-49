            <rdg wit="#O">qua</rdg> <!-- This must be an error for aliqua -->
            <rdg wit="#B" type="variation-absent"/>
          </app>
          <app>
            <rdg wit="#O">enim</rdg>
            <rdg wit="#B">autem</rdg>
          </app>
          sit
          <app>
            <rdg wit="#O" type="variation-absent"/>
            <rdg wit="#B">aliqua</rdg>
          </app>
          consideratio
          <app>
            <rdg wit="#O">abstracta</rdg>
            <rdg wit="#B" type="variation-absent"/>
          </app>
          per
          <!-- In this section O seems better. Well, that's a theological
               question. Does inspiration extend or exceed metaphysics??? Is the
               extension Avicennean? Are theology and ontology different? -->
          <app>
            <rdg wit="#O">inspirationem</rdg>
            <rdg wit="#B">aspirationem</rdg>
          </app>
          acquisita
          <app>
            <rdg wit="#O">ipsa extendit</rdg>
            <rdg wit="#B">excedit</rdg>
          </app>
          considerationem metaphysici
          <app>
            <rdg wit="#O">sed talis non est praesentis speculationis</rdg>
            <rdg wit="#B" type="variation-absent"/>
          </app>
          sic
          <app>
            <rdg wit="#O">igitur</rdg>
            <rdg wit="#B">ergo</rdg>
          </app>
          intelligenda sunt quae dicta sunt quod cum tres sint partes
          <app>
            <rdg wit="#O">essentiales philosophae</rdg>
            <rdg wit="#B" type="variation-inversion">philosophiae essentiales</rdg>
          </app>
          scilicet naturalis, mathematica et divina inter
          <app>
            <rdg wit="#O">artes vel</rdg>
            <rdg wit="#B" type="variation-absent"/>
          </app>
          scientias per artem humanam inventas
          <app>
            <!-- On this O prefers personal, B abstract. Both work. -->
            <rdg wit="#O">metaphysicus</rdg>
            <rdg wit="#B">metaphysica</rdg>
          </app>
          non recognoscit a materia scientiam superiorem physica etiam inter
          partes physicae essentiales quem ad modum abstractionis non
          recognoscit a
          <app>
            <!-- B better -->
            <rdg wit="#O">materiae artificem</rdg>
            <rdg wit="#B">materia scientiam</rdg>
          </app>
          <app>
            <!-- B better -->
            <rdg wit="#O"><unclear>licet</unclear> superiorem</rdg>
            <rdg wit="#B">sed superiorem recognoscentem recognoscit</rdg>
          </app>
          <app>
            <rdg wit="#O">sed</rdg>
            <rdg wit="#B" type="variation-absent"/>
          </app>
          <!-- In this sentence they are equal -->
          <app>
            <rdg wit="#O">mathematicus</rdg>
            <rdg wit="#B">mathematica</rdg>
          </app>
          recognoscit et
          <app>
            <rdg wit="#O">
              artificem superiorem quia aliquis est qui magis abstrahit
            </rdg>
            <rdg wit="#B">
              superiorem
            </rdg>
          </app>
          et inferiorem quia
          <app>
            <rdg wit="#O">aliquis est qui</rdg>
            <rdg wit="#B">aliqua est quae magis abstrahit et aliqua quae</rdg>
          </app>
          magis concrevit unde ut dictum est consideratio
          <app>
            <rdg wit="#O">physici</rdg>
            <rdg wit="#B">physica</rdg>
          </app>
          incipit a formis infimis et terminatur ad animam
          <app>
            <rdg wit="#O">humanam et</rdg>
            <rdg wit="#B" type="variation-absent"/>
          </app>
          ex hoc patet quod liber de anima
          <app>
            <rdg wit="#O">sextum gradum tenet</rdg>
            <rdg wit="#B">tenet sectum gradum</rdg>
          </app>
          inter libros naturales quia primum gradum tenet liber physicorum
          <app>
            <rdg wit="#O" type="variation-absent"/>
            <rdg wit="#B">qui est de naturali audito</rdg>
          </app>
          secundum
          <app>
            <rdg wit="#O" type="variation-absent"/>
            <rdg wit="#B">tenet</rdg>
          </app>
          liber de caelo et mundo tertium
          <app>
            <rdg wit="#O" type="variation-absent"/>
            <rdg wit="#B">gradum tenet</rdg>
          </app>
          <app>
            <rdg wit="#O" type="correction-addition"><add place="above-line">liber</add></rdg>
            <rdg wit="#B">liber</rdg>
          </app>
          de generatione
          <app>
            <rdg wit="#O">et <cb ed="#O" n="b"/> corruptione</rdg>
            <rdg wit="#B" type="variation-absent"/>
          </app>
          quartum gradum tenet liber meteorum quintum
          <app>
            <rdg wit="#O" type="variation-absent"/>
            <rdg wit="#B">gradum tenet</rdg>
          </app>
          liber de <unclear>mineris/mineralibus</unclear> sextum
          <app>
            <rdg wit="#O" type="variation-absent"/>
            <rdg wit="#B">gradum tenet</rdg>
          </app>
          liber de anima
          <app>
            <!-- B better -->
            <rdg wit="#O">unde primo est iste liber</rdg>
            <rdg wit="#B">qui prior est</rdg>
          </app>
          dignitate
          <app>
            <rdg wit="#O">licet ultimus sive</rdg>
            <rdg wit="#B" type="variation-absent"/>
          </app>
          sextus
          <app>
            <rdg wit="#O" type="variation-absent"/>
            <rdg wit="#B">tamen</rdg>
          </app>
          ordine doctrinae
          <app>
            <rdg wit="#O">et ideo</rdg>
            <rdg wit="#B">unde</rdg>
          </app>
          Philosophus
          <app>
            <rdg wit="#O">considerans eius dignitatem</rdg>
            <rdg wit="#B">in prohemio huius primi</rdg>
          </app>
          dicit quod historia de anima
          <app>
            <rdg wit="#O">in primis</rdg>
            <rdg wit="#B">primo</rdg>
          </app>
          est ponenda
          <app>
            <!-- B again more extensive -->
            <rdg wit="#O">
              <!-- Is "consideratio de anima physica" better? -->
              quia ergo supponitur quod consideratio physica de anima est
            </rdg>
            <rdg wit="#B">
              scilicet dignitate unde supposita communi distinctione librorum
              naturalium alibi ponita quaerantur primo quaedam communia circa
              librum de anima et secundum magis in speciali
            </rdg>
          </app>
          .
        </p>
      </div>
